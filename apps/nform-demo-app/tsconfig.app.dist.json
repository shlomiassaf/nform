      "@pebula/apps/*": ["apps/libs/*", "apps/libs/*/src/index.ts"],
      "@pebula/utils/lib/*": ["dist/@pebula/utils/lib/*"],
      "@pebula/metap/lib/*": ["dist/@pebula/metap/lib/*"],
      "@pebula/nform/lib/*": ["dist/@pebula/nform/lib/*"],
      "@pebula/nform-material/lib/*": ["dist/@pebula/nform-material/lib/*"],
      "@pebula/*": ["dist/@pebula/*"]
    }
  }
}
